#!/bin/bash

if [ "${SCRIPT_PATH}" == "" ]; then
    echo "This script should not be called directly! Please use mq.sh"
    exit -1
fi

function SystemNumFiles_odroid() {
    echo 1
}

function SystemRunImage_odroid() {
    local completion=$1
    local completion_timeout=$2
    local errortxt=$3
    local output=$4
    local kernel=$5
    RebootConsoleRunOneFile "${completion}" "${completion_timeout}" "${errortxt}" "${output}" "${kernel}" odroid
    return $?
}

function SystemPowerOff_odroid() {
    RebootShutdown odroid
}

function Arch_odroid() {
    echo "arm"
}

function ISA_odroid() {
    echo "armv7-a"
}

function SOC_odroid() {
    echo "exynos4412"
}

function CPU_odroid() {
    echo "cortex-a9"
}

function Cores_odroid() {
    echo "4"
}

function RAM_odroid() {
    echo "1GB"
}

function MaxCLK_odroid() {
    echo "1.4Ghz"
}


