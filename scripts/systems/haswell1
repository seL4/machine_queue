#!/bin/sh

if [ "${SCRIPT_PATH}" = "" ]; then
    echo "This script should not be called directly! Please use mq.sh"
    exit -1
fi

SystemNumFiles_haswell1() {
    echo 2
}

SystemName_haswell1() {
    echo "haswell1"
}

SystemPowerOff_haswell1() {
    RebootShutdown haswell1
}

Arch_haswell1() {
    echo "x86"
}

ISA_haswell1() {
    echo "x86_64"
}

SOC_haswell1() {
    echo "haswell"
}

CPU_haswell1() {
    echo "E3-1220v3"
}

Cores_haswell1() {
    echo "4(8)"
}

RAM_haswell1() {
    echo "?"
}

MaxCLK_haswell1() {
    echo "3.1Ghz"
}

Sel4Plat_haswell1() {
    echo "pc99:haswell"
}
