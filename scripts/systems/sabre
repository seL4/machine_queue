#!/bin/bash

if [ "${SCRIPT_PATH}" == "" ]; then
    echo "This script should not be called directly! Please use mq.sh"
    exit -1
fi

function SystemNumFiles_sabre() {
    echo 1
}

function SystemRunImage_sabre() {
    local completion=$1
    local completion_timeout=$2
    local output=$3
    local kernel=$4
    RebootConsoleRunOneFile "${completion}" "${completion_timeout}" "${output}" "${kernel}" sabre
    return $?
}

function SystemPowerOff_sabre() {
    RebootShutdown sabre
}

function Arch_sabre() {
    echo "arm"
}

function ISA_sabre() {
    echo "armv7-a"
}

function SOC_sabre() {
    echo "imx6"
}

function CPU_sabre() {
    echo "cortex-a9"
}

function Cores_sabre() {
    echo "4"
}

function RAM_sabre() {
    echo "1GB"
}

function MaxCLK_sabre() {
    echo "1Ghz"
}


