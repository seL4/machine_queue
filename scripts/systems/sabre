#!/bin/bash

if [ "${SCRIPT_PATH}" == "" ]; then
    echo "This script should not be called directly! Please use mq.sh"
    exit -1
fi

function SystemNumFiles_sabre() {
    echo 1
}

function SystemName_sabre() {
    echo "sabre"
}

function SystemPowerOff_sabre() {
    RebootShutdown sabre
}

function Arch_sabre() {
    echo "arm"
}

function ISA_sabre() {
    echo "armv7-a"
}

function SOC_sabre() {
    echo "imx6"
}

function CPU_sabre() {
    echo "cortex-a9"
}

function Cores_sabre() {
    echo "4"
}

function RAM_sabre() {
    echo "1GB"
}

function MaxCLK_sabre() {
    echo "1Ghz"
}

function Sel4Plat_sabre() {
    echo "imx6:sabre"
}
