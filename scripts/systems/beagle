#!/bin/sh

if [ "${SCRIPT_PATH}" = "" ]; then
    echo "This script should not be called directly! Please use mq.sh"
    exit -1
fi

SystemNumFiles_beagle() {
    echo 1
}

SystemName_beagle() {
    echo "beagle1"
}

SystemPowerOff_beagle() {
    RebootShutdown beagle
}

Arch_beagle() {
    echo "arm"
}

ISA_beagle() {
    echo "armv7-a"
}

SOC_beagle() {
    echo "omap3"
}

CPU_beagle() {
    echo "cortex-a9"
}

Cores_beagle() {
    echo "1"
}

RAM_beagle() {
    echo "256MB"
}

MaxCLK_beagle() {
    echo "720Mhz"
}

Sel4Plat_beagle() {
    echo "omap3"
}

PXELinux_beagle() {
    echo "no"
}

DTB_beagle() {
    echo "no"
}

