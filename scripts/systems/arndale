#!/bin/bash

if [ "${SCRIPT_PATH}" == "" ]; then
    echo "This script should not be called directly! Please use mq.sh"
    exit -1
fi

function SystemNumFiles_arndale() {
    echo 1
}

function SystemRunImage_arndale() {
    local completion=$1
    local completion_timeout=$2
    local errortxt=$3
    local output=$4
    local kernel=$5
    RebootConsoleRunOneFile "${completion}" "${completion_timeout}" "${errortxt}" "${output}" "${kernel}" arndale
    return $?
}

function SystemPowerOff_arndale() {
    RebootShutdown arndale
}

function Arch_arndale() {
    echo "arm"
}

function ISA_arndale() {
    echo "armv7-a"
}

function SOC_arndale() {
    echo "exynos5"
}

function CPU_arndale() {
    echo "cortex-a15"
}

function Cores_arndale() {
    echo "2"
}

function RAM_arndale() {
    echo "2GB"
}

function MaxCLK_arndale() {
    echo "1.7GHz"
}